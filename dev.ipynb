    "from day_15 import Day15, Move\n",
    "\n",
    "\n",
    "day = Day15(\"./input/day_15.txt\")\n",
    "grid = day.grid\n",
    "r, c = day.start\n",
    "\n",
    "for move in day.moves:\n",
    "    print(r,c, move)\n",
    "    dr, dc = day.move(r,c, move,grid)\n",
    "    r += dr\n",
    "    c += dc\n",
    "    print(day.print_grid(grid))\n",
    "\n"
   ]
